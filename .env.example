# Supabase Configuration Example
# Copy this file to .env and replace with your actual values

# Your Supabase project URL (e.g., https://your-project-id.supabase.co)
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Google OAuth client IDs (Expo AuthSession)
# These are not secrets but should be managed via env for flexibility.
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# Redirect scheme used by makeRedirectUri (defaults to zenlit-native if omitted)
EXPO_PUBLIC_REDIRECT_SCHEME=zenlit-native